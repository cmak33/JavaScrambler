<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="511.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.scrambler.MainController">
   <children>
      <Button fx:id="choseInputFileButton" layoutX="81.0" layoutY="93.0" mnemonicParsing="false" onAction="#onChoseInputButtonClick" prefHeight="82.0" prefWidth="109.0" text="Chose input file" />
      <Button fx:id="choseOutputFileButton" layoutX="325.0" layoutY="91.0" mnemonicParsing="false" onAction="#onChoseOutputButtonClick" prefHeight="82.0" prefWidth="116.0" text=" Chose output file" />
      <Label fx:id="inputFilePathLabel" layoutX="79.0" layoutY="191.0" prefHeight="18.0" prefWidth="110.0" />
      <Label fx:id="outputFilePathLabel" layoutX="328.0" layoutY="191.0" prefHeight="18.0" prefWidth="110.0" />
      <Button fx:id="encryptButton" layoutX="164.0" layoutY="282.0" mnemonicParsing="false" onAction="#encrypt" text="Encrypt" />
      <Button fx:id="decryptButton" layoutX="290.0" layoutY="282.0" mnemonicParsing="false" onAction="#decrypt" text="Decrypt" />
      <Label fx:id="profileLabel" layoutX="427.0" layoutY="24.0" onMouseClicked="#switchToProfileView" prefHeight="18.0" prefWidth="58.0" text="Profile" />
      <Label fx:id="errorLabel" layoutX="200.0" layoutY="237.0" prefHeight="18.0" prefWidth="110.0" />
   </children>
</AnchorPane>
